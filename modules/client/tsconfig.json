{
  "compilerOptions": {
    "baseUrl": "./",
    "typeRoots": [
      "node_modules/@types",
      "node_modules"
    ],
    "types": [ "babylonjs", "inversify", "lodash"],
    "target": "es6",
    "lib": [
      "es6",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "traceResolution" : true
  },
   "exclude": [
    "!../../node_modules/@types"
  ],
  "modules": null
}